<h1><%= resourceBundles.getString("Contact Irish Dictionary Online") %></h1>
<div>
  <div style="background-color: #F5F5F5; padding: 33px; margin: 33px; border-radius: 3px; border: #e2e2e2 1px solid;">
    <form style="font-size: 16px;" action="contact" method="post">
      <div style="border: #d5d5d5 1px solid; border-radius: 0px; margin: 10px; padding: 3px 3px; background-color: #fff;">
        <input type="text" name="name" id="name" value="" tabindex="1" placeholder="Name..." style="width: 100%; padding: 0px; margin: 0px; border: 0px solid; font-size: 15px;">
      </div>
      <div style="border: #e2e2e2 1px solid; border-radius: 0px; margin: 10px; padding: 3px 3px; background-color: #fff;">
        <input type="email" name="name" id="name" value="" tabindex="2" placeholder="Email..." style="width: 100%; padding: 0px; margin: 0px; border: 0px solid; font-size: 15px;">
      </div>
      <div style="border: #ccc 1px solid; border-radius: 0px; margin: 10px; padding: 3px; background-color: #fff;">
        <textarea cols="40" rows="6" name="textarea" id="textarea" placeholder="Your message..." style="width: 100%; padding: 0px; margin: 0px; border: 0px solid; font-size: 15px; font-family: verdana, courier, arial;"></textarea>
      </div>
      <div style="margin: 10px;">
        <button type="submit" style="border: #d5d5d5 1px solid; background-color: #e2e2e2; color: #2d2d2d; padding: 5px 25px; font-weight: bold; font-size: 18px; font-family: arial; ">Send</button>
      </div>
    </form>
  </div>
</div>
